{
  "name": "npm-version-check-action",
  "description": "GitHub Action that validates npm package version increments in pull requests to ensure proper semantic versioning",
  "version": "1.0.3",
  "type": "module",
  "author": {
    "name": "Josh Johanning",
    "email": "joshjohanning@github.com",
    "url": "https://github.com/joshjohanning"
  },
  "private": true,
  "homepage": "https://github.com/joshjohanning/npm-version-check-action#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshjohanning/npm-version-check-action.git"
  },
  "bugs": {
    "url": "https://github.com/joshjohanning/npm-version-check-action/issues"
  },
  "keywords": [
    "github-action",
    "npm",
    "version",
    "semantic-versioning",
    "pull-request",
    "validation",
    "package-json"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . -c ./.github/linters/.eslintrc.yml",
    "package": "npx @vercel/ncc build src/index.js -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "test": "npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.54.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2"
  }
}
